<resources>
    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">#FFFFFF</item>
        <item name="colorPrimaryDark">#FFFFFF</item>
        <item name="colorAccent">@color/serachbar_backround</item>
        <item name="floatingActionButtonStyle">@style/fab</item>
    </style>

    <style name="fab" parent="Widget.Design.FloatingActionButton">
        <item name="shapeAppearanceOverlay">@style/fabStyle</item>

    </style>

    <style name="fabStyle" parent="ShapeAppearance.MaterialComponents.SmallComponent">
        <item name="cornerSize">50%</item>
        <item name="cornerFamily">rounded</item>

    </style>

</resources>